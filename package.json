{
  "name": "zi-start-basic",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000 --host",
    "dev:wx-test": "vite dev --port 3000 --host --mode wx-test",
    "build": "pnpm run build:css:legacy && vite build",
    "build:css:legacy": "cross-env BROWSERSLIST_ENV=legacy postcss src/styles/index.css -o src/styles/index.legacy.css",
    "preview": "vite preview --host",
    "start": "bun run server.ts",
    "start:win": "bun run server.ts",
    "start:debug": "ENABLE_PERF_LOGGING=true ASSET_PRELOAD_VERBOSE_LOGGING=true bun run server.ts",
    "start:verbose": "ASSET_PRELOAD_VERBOSE_LOGGING=true bun run server.ts",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:start:dev": "pm2 start ecosystem.config.cjs --env development",
    "pm2:stop": "pm2 stop zi-start-basic",
    "pm2:restart": "pm2 restart zi-start-basic",
    "pm2:reload": "pm2 reload zi-start-basic",
    "pm2:delete": "pm2 delete zi-start-basic",
    "pm2:logs": "pm2 logs zi-start-basic",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status zi-start-basic",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "generate:routes": "pnpm exec tsx scripts/generate-routes.ts",
    "prisma:schema:merge": "pnpm exec tsx scripts/merge-prisma-schema.ts",
    "prisma:generate": "pnpm run prisma:schema:merge && pnpm exec prisma generate",
    "prisma:migrate:dev": "pnpm run prisma:schema:merge && pnpm exec prisma migrate dev",
    "prisma:migrate:deploy": "pnpm run prisma:schema:merge && pnpm exec prisma migrate deploy",
    "lint": "eslint .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lint-staged": "lint-staged",
    "check-types": "pnpm run generate:routes && tsc --noEmit",
    "check": "pnpm run format && pnpm run lint && pnpm run check-types"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,mjs,cjs,json,md,css,scss,yml,yaml}": [
      "prettier --write"
    ]
  },
